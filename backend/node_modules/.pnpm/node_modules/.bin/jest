#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Applications/glucoheart/backend/node_modules/.pnpm/jest-cli@30.0.5_@types+node_68ffafd34d06a981df35f0742f898a76/node_modules/jest-cli/bin/node_modules:/mnt/d/Applications/glucoheart/backend/node_modules/.pnpm/jest-cli@30.0.5_@types+node_68ffafd34d06a981df35f0742f898a76/node_modules/jest-cli/node_modules:/mnt/d/Applications/glucoheart/backend/node_modules/.pnpm/jest-cli@30.0.5_@types+node_68ffafd34d06a981df35f0742f898a76/node_modules:/mnt/d/Applications/glucoheart/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Applications/glucoheart/backend/node_modules/.pnpm/jest-cli@30.0.5_@types+node_68ffafd34d06a981df35f0742f898a76/node_modules/jest-cli/bin/node_modules:/mnt/d/Applications/glucoheart/backend/node_modules/.pnpm/jest-cli@30.0.5_@types+node_68ffafd34d06a981df35f0742f898a76/node_modules/jest-cli/node_modules:/mnt/d/Applications/glucoheart/backend/node_modules/.pnpm/jest-cli@30.0.5_@types+node_68ffafd34d06a981df35f0742f898a76/node_modules:/mnt/d/Applications/glucoheart/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-cli/bin/jest.js" "$@"
fi
